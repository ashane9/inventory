<p id="notice"><%= notice %></p>

<h1>Autographs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Authentication</th>
      <th>Authentication number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autographs.each do |autograph| %>
      <tr>
        <td><%= autograph.name %></td>
        <td><%= autograph.authentication %></td>
        <td><%= autograph.authentication_number %></td>
        <td><%= link_to 'Show', autograph %></td>
        <td><%= link_to 'Edit', edit_autograph_path(autograph) %></td>
        <td><%= link_to 'Destroy', autograph, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Autograph', new_autograph_path %>
